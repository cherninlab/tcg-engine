{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"lib": ["ES2020"],
		"types": ["@cloudflare/workers-types", "./worker-configuration.d.ts"],
		"skipLibCheck": true,
		"noEmit": true,
		"isolatedModules": true,
		"allowJs": true,
		"outDir": "./dist"
	},
	"include": ["src"]
}
